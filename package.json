{
  "name": "type-convert",
  "version": "1.1.1",
  "description": "Given a value, convert the remaining values to its type",
  "main": "index.js",
  "scripts": {
    "prerelease": "npm test && standard-version -s",
    "release": "node -r dotenv/config ./node_modules/.bin/conventional-github-releaser -p angular",
    "postrelease": "git push --follow-tags && npm publish",
    "pretest": "xo",
    "test": "nyc ava"
  },
  "author": "Jason Kurian <jgkurian@me.com>",
  "bugs": {
    "url": "https://github.com/JaKXz/type-convert/issues"
  },
  "homepage": "https://github.com/JaKXz/type-convert#readme",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.16.0",
    "conventional-github-releaser": "^1.1.3",
    "coveralls": "^2.11.11",
    "dotenv": "^2.0.0",
    "nyc": "^9.0.1",
    "standard-version": "^3.0.0",
    "testdouble": "^1.6.0",
    "xo": "^0.17.1"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "branches": 100,
    "functions": 100,
    "statements": 100,
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "xo": {
    "space": true,
    "rules": {
      "quote-props": [
        "error",
        "consistent"
      ],
      "babel/arrow-parens": [
        "error",
        "always"
      ]
    }
  }
}
